{
  "name": "alt-demo",
  "version": "1.0.0",
  "description": "Alt.js Demo...",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TheBrownSound/alt-spaceship.git"
  },
  "keywords": [
    "Alt",
    "Flux",
    "Demo"
  ],
  "author": "Jarad Comingdeer",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/TheBrownSound/alt-spaceship/issues"
  },
  "homepage": "https://github.com/TheBrownSound/alt-spaceship#readme",
  "dependencies": {
    "alt": "^0.18.4",
    "color": "^0.11.1",
    "react": "^0.14.8",
    "react-dom": "^0.14.8"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "jade": "^1.11.0"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "watch": {
    "build:js": {
      "patterns": [
        "source"
      ],
      "extensions": "js,jsx"
    },
    "build:html": "source/**/*.jade"
  },
  "scripts": {
    "clean": "rm public/index.html & rm public/app.js",
    "build:js": "browserify source/demo/app.js -o public/app.js --extension='.jsx'",
    "build:html": "jade source/demo/index.jade -o public/",
    "build": "npm run build:js && npm run build:html",
    "watch": "npm-watch"
  }
}
